<%- include("partials/header.ejs")  %>
<h1 class="title">Welcome To Tlikiner</h1>
  <div class="container">
    
    <div class="posts">
      <h2>Posts</h2>
      <% if(locals.tlikini) { %> 
        <% for(i=0; i<kargistyvna.length; i++) { %>
          <div class="post">
            <p><%= kargistyvna[i]%></p>
            <div class="buttons">
              <a href="/" class="delete-btn"><i class="fa-solid fa-trash"></i></a>
              <a href="/" class="edit-btn"><i class="fa-solid fa-pen-to-square"></i></a>
            </div>
          </div>
        <%}; %>
        <% } %>
      </div>
    </div>
    
<%- include("partials/footer.ejs")  %>



